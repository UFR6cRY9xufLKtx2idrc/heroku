{
    "dns": {
        "servers": [
            "https+local://8.8.8.8/dns-query",
            "https+local://8.8.4.4/dns-query"
        ]
    },
    "inbounds": [
        {
            "listen": "/etc/caddy/vmess",
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "$AUUID"
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "earlyDataHeaderName": "Sec-WebSocket-Protocol",
                    "maxEarlyData": 4096,
                    "path": "/$AUUID-vmess"
                }
            }
        },
        {
            "listen": "/etc/caddy/vless",
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "encryption": "none",
                        "id": "$AUUID"
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "earlyDataHeaderName": "Sec-WebSocket-Protocol",
                    "maxEarlyData": 4096,
                    "path": "/$AUUID-vless"
                }
            }
        },
        {
            "listen": "/etc/caddy/trojan",
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password": "$AUUID"
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/$AUUID-trojan"
                }
            }
        },
        {
            "listen": "127.0.0.1",
            "port": 4234,
            "protocol": "dokodemo-door",
            "settings": {
                "address": "v1.mux.cool",
                "followRedirect": false,
                "network": "tcp"
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "path": "/$AUUID-ss"
                }
            },
            "tag": "onetag"
        },
        {
            "listen": "127.0.0.1",
            "port": 4324,
            "protocol": "shadowsocks",
            "settings": {
                "method": "$ParameterSSENCYPT",
                "password": "$AUUID"
            },
            "streamSettings": {
                "dsSettings": {
                    "abstract": true,
                    "path": "apath"
                },
                "network": "domainsocket",
                "security": "none"
            }
        },
        {
            "listen": "127.0.0.1",
            "port": 5234,
            "protocol": "socks",
            "settings": {
                "accounts": [
                    {
                        "pass": "$AUUID",
                        "user": "$AUUID"
                    }
                ],
                "auth": "password"
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/$AUUID-socks"
                }
            }
        }
    ],
    "log": {
        "loglevel": "none"
    },
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "UseIP"
            },
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "settings": {
            },
            "tag": "blocked"
        },
        {
            "protocol": "socks",
            "settings": {
                "servers": [
                    {
                        "address": "127.0.0.1",
                        "port": 9050
                    }
                ]
            },
            "tag": "sockstor"
        },
        {
            "protocol": "freedom",
            "streamSettings": {
                "dsSettings": {
                    "abstract": true,
                    "path": "apath"
                },
                "network": "domainsocket"
            },
            "tag": "twotag"
        }
    ],
    "routing": {
        "domainMatcher": "mph",
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "inboundTag": [
                    "onetag"
                ],
                "outboundTag": "twotag",
                "type": "field"
            },
            {
                "domain": [
                    "geosite:tor"
                ],
                "outboundTag": "sockstor",
                "type": "field"
            },
            {
                "domain": [
                    "geosite:category-ads-all",
                    "geosite:private"
                ],
                "ip": [
                    "geoip:private"
                ],
                "outboundTag": "blocked",
                "type": "field"
            }
        ]
    }
}
